#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="$basedir/D:/learn/github/Angular/node_modules/.store/@npmcli+installed-package-contents@2.1.0/node_modules/@npmcli"
else
  export NODE_PATH="$NODE_PATH:$basedir/D:/learn/github/Angular/node_modules/.store/@npmcli+installed-package-contents@2.1.0/node_modules/@npmcli"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../@npmcli+installed-package-contents@2.1.0/node_modules/@npmcli/installed-package-contents/bin/index.js" "$@"
else
  exec node  "$basedir/../../../../../@npmcli+installed-package-contents@2.1.0/node_modules/@npmcli/installed-package-contents/bin/index.js" "$@"
fi
